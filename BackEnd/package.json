  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check-upgrade-nest": "ncu",
    "upgrade-nest": "ncu -u && npm install",
    "publish": "npm run build && rm .env && cp live.env .env && rm .htaccess && cp live.htaccess .htaccess && cd ../WebApp && npm run publish",
    "publish_uat": "npm run build && rm .env && cp UAT.env .env",
    "start_host": "pm2 start dist/main.js -n JS",
    "delete_host": "pm2 delete JS"
  },
